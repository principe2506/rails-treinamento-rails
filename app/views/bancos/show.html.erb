<div class="page-header">
  <%= link_to bancos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_banco_path(@banco), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show banco</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @banco.nome %></dd>

  <dt>Localizacao:</dt>
  <dd><%= @banco.localizacao %></dd>

  <dt>Ano fundacao:</dt>
  <dd><%= @banco.ano_fundacao %></dd>

  <dt>Numero agencias:</dt>
  <dd><%= @banco.numero_agencias %></dd>

  <dt>Email:</dt>
  <dd><%= @banco.email %></dd>

  <dt>Obs:</dt>
  <dd><%= @banco.obs %></dd>

</dl>
